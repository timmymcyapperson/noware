-- Scripts:

local function toggle1(name, callback1, callback2)
	local TextButton = Instance.new("TextButton")
	TextButton.Parent = Combat
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.BorderSizePixel = 0
	TextButton.Position = UDim2.new(0, 0, 0.102459013, 0)
	TextButton.Size = UDim2.new(0, 206, 0, 50)
	TextButton.Font = Enum.Font.FredokaOne
	TextButton.Text = name
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextSize = 20.000

	local state = false -- Initial state

	TextButton.MouseButton1Click:Connect(function()
		if state then
			callback1()
			state = false
		else
			callback2()
			state = true
		end

		Sound:Play() -- Play the sound when the button is clicked
	end)
end

local function button1(name, callback)
	local TextButton = Instance.new("TextButton")
	TextButton.Parent = Combat
	TextButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.BorderSizePixel = 0
	TextButton.Position = UDim2.new(0, 0, 0.204918027, 0) -- Adjusted position to prevent overlap
	TextButton.Size = UDim2.new(0, 206, 0, 50)
	TextButton.Font = Enum.Font.FredokaOne
	TextButton.Text = name
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextSize = 20.000

	TextButton.MouseButton1Click:Connect(function()
		callback()
		Sound:Play() -- Play the sound when the button is clicked
	end)
end

