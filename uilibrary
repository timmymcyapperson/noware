local frames = {Combat, Blatant, Uitilities, World} -- Assuming these frames are already defined in your script

local function toggleFramesVisibility()
	for _, frame in ipairs(frames) do
		frame.Visible = not frame.Visible
	end
end

game:GetService("UserInputService").InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.RightShift and input.UserInputType == Enum.UserInputType.Keyboard then
		toggleFramesVisibility()
	end
end)
